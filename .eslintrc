{
    "extends": "airbnb-base",
    "rules": {
        "indent": [ "error", 4 ],
        "import/no-extraneous-dependencies": false,
        "prefer-promise-reject-errors": 0,
        "import/no-unresolved": [
            "error",
            { "ignore": ["#"] }
        ],
        "max-len": "off",
        "no-param-reassign": "warn"
    },
    "env": {
        "browser": true
    },
    "globals": {
        "App": "readonly"
    }
}